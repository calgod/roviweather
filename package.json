{
  "name": "rovisys-weather-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "npm run test:frontend && npm run test:worker",
    "test:frontend": "vitest run",
    "test:frontend:watch": "vitest",
    "test:worker": "npm --prefix worker test",
    "worker:dev": "npm --prefix worker run dev",
    "worker:deploy": "npm --prefix worker run deploy"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "lucide-react": "^0.574.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/leaflet": "^1.9.14",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "jsdom": "^28.1.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^5.4.11",
    "vitest": "^4.0.18"
  }
}
